extends ../layouts/application.pug

block content
  .container.mt-4.h-100
    h1 Список пользователей
    table.table.table-striped
      thead
        tr
          th= t('users.id')
          th= t('users.email')
          th= t('users.createdAt')
      tbody
        each user in users
          tr
            td= user.id
            td= user.email
            td= user.createdAt
